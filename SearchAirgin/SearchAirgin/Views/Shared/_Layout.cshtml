<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>@ViewData["Title"] - Oknaa Airlines</title>
    <link rel="stylesheet" href="assets/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Cabin:700">
    <link rel="stylesheet" href="assets/fonts/font-awesome.min.css" asp-append-version="true">
    <link rel="stylesheet" href="assets/css/Bootstrap-DataTables.css" asp-append-version="true">
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.20/css/dataTables.bootstrap4.min.css"
          crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css">
</head>

<body id="page-top" data-bs-spy="scroll" data-bs-target="#mainNav" data-bs-offset="77"
      style="background: url(&quot;assets/img/IMG_7461.jpg&quot;);">
<header class="masthead" style="background: transparent;">
    <div class="intro-body" style="height: 900px;">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 mx-auto" style="width: 1078px;">
                    <img class="pulse animated"
                         src="assets/img/OKNAA%20AIRLINES%20Logo%20-%20Original%20with%20Transparent%20Background(3).svg"
                         style="width: 1000px;margin-bottom: -249px;margin-top: -500px;padding-bottom: -60px;">
                </div>
            </div>
        </div>
    </div>
</header>
<section class="text-center content-section" id="about" style="padding: 0px;">
    <div class="row padMar">
        <div class="col padMar">
            <div class="input-group"></div>
        </div>
    </div>
    <div class="container">
        <div class="row" style="margin: 0px;">
            @RenderBody()
        </div>
    </div>
</section>
<div class="map-clean"></div>
<footer>
    <div class="container text-center">
        <p>Copyright ©&nbsp;Brand 2022</p>
    </div>
</footer>
<script src="assets/bootstrap/js/bootstrap.min.js"></script>
<script src="assets/js/bs-init.js"></script>
<script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js"></script>
<script src="https://cdn.datatables.net/1.10.20/js/dataTables.bootstrap4.min.js"></script>
<script src="assets/js/Bootstrap-DataTables.js"></script>
<script src="assets/js/grayscale.js"></script>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", false)
</body>

</html>